{
  "name": "orizen-flow",
  "version": "0.0.13",
  "private": true,
  "homepage": "https://github.com/paras-verma7454/orizenflow#readme",
  "bugs": "https://github.com/paras-verma7454/orizenflow/issues",
  "license": "MIT",
  "repository": "paras-verma7454/orizenflow",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build --summarize",
    "check-types": "turbo run check-types --summarize",
    "clean": "bunx rimraf --glob \"**/.next\" \"**/.turbo\" \"**/dist\" \"**/node_modules\" && bunx delete-empty",
    "db:generate": "turbo run build --filter=@packages/env && bun --cwd packages/db drizzle-kit generate",
    "db:migrate": "turbo run build --filter=@packages/env && bun --cwd packages/db drizzle-kit migrate",
    "db:push": "turbo run build --filter=@packages/env && bun --cwd packages/db drizzle-kit push",
    "db:studio": "turbo run build --filter=@packages/env && bun --cwd packages/db drizzle-kit studio",
    "dev": "turbo run dev --ui tui",
    "devtools": "turbo devtools",
    "format": "oxfmt",
    "format:check": "oxfmt --check",
    "postinstall": "bun .github/scripts/deps-manager.ts",
    "lint": "turbo run lint --summarize",
    "prepare": "bunx lefthook install",
    "shadcn:update": "bash .github/scripts/shadcn-update.sh",
    "start": "turbo run start"
  },
  "devDependencies": {
    "@commitlint/cli": "catalog:",
    "@commitlint/config-conventional": "catalog:",
    "@octokit/rest": "catalog:",
    "@types/bun": "catalog:",
