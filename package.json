    "@typescript/native-preview": "catalog:",
    "drizzle-kit": "^0.31.9",
    "globby": "catalog:",
    "lefthook": "catalog:",
    "lint-staged": "catalog:",
    "oxfmt": "catalog:",
    "oxlint": "catalog:",
    "tsx": "catalog:",
    "turbo": "catalog:"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "packageManager": "bun@1.3.7",
